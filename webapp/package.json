{
  "name": "face-webapp",
  "version": "1.0.0",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "node app.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "aws",
    "rekognition",
    "s3",
    "mysql",
    "express"
  ],
  "author": "Face Recognition Lab",
  "license": "ISC",
  "description": "Interface Express pour charger des visages vers S3 et les visualiser depuis la base MySQL",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.933.0",
    "express": "^5.1.0",
    "multer": "^2.0.2",
    "mysql2": "^3.15.3"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "@types/jest": "^29.5.11"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/tests/"
    ],
    "testMatch": [
      "**/tests/**/*.test.js"
    ]
  }
}
